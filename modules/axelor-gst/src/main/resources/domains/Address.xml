<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
          http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
     
     <module package="com.axelor.gst.db" name="gst"/> 
      <entity name="Address">
         <string name="type" title="Type" selection="select.gst.address.type" required="true"/>
          <string name="line1" title="Line1" required="true"/>
          <string name="line2" title="Line2"/>
          <many-to-one name="city" ref="City" title="City" required="true"/>
          <many-to-one name="state" ref="State" title="State"/>
          <many-to-one name="country" ref="Country" title="Country" required="true" />
     
     
      </entity>  
          
  </domain-models>        
          