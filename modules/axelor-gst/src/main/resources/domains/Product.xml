<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
          http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
     
     <module package="com.axelor.gst.db" name="gst"/>
     
       <entity name="Product">
         
         <string name="name" title="Name" required="true"/>
         <string name="code" title="Code" required="true"/>
         <string name="hsbn" title="HSBN" />
         <many-to-one name="category" ref="ProductCategory"/>
         <decimal name="salePrice" title="Sale price"/>
         <decimal name="costPrice" title="Cost price"/>
         <binary name="image" title="Image"/>
         <decimal name="gstRate" title="GST rate" default="00.0"/>
         
       </entity>
     
   </domain-models>  