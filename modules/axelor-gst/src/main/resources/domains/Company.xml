<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
          http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
      
     <module package="com.axelor.gst.db" name="gst"/>
     
     <entity name="Company">
        <string name="name" title="Name" required="true"/>
        <many-to-one ref="com.axelor.meta.db.MetaFile" name="logo" title="Logo"/>
        <one-to-many ref="Contact" name="contact"/>
        <string name="gstin" title="GSTIN" min="15" max="15"/>
        <one-to-many ref="BankAccount" name="bankDetails"/>
        <many-to-one ref="Address" name="address" title="Address"/>
        <!--  <many-to-one ref="MetaModel" name="model" title="Model"/>  -->
     </entity>
          
 </domain-models>         