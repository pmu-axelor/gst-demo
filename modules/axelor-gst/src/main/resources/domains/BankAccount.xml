<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
          http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
     
     <module package="com.axelor.gst.db" name="gst"/> 
     
     <entity name="BankAccount">
     
        <string name="bank" title="Bank" namecolumn="true"/>
        <string name="accountNo" title="Account no"/>
        <string name="ifsc" title="IFSC"/>
        <string name="type" title="Type" selection="gst.bank-account.gst.type.select" default="current"/>
        <string name="address" title="Address" large="true"/>
         
     </entity>
 </domain-models>       