<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
          http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">
     
     <module package="com.axelor.gst.db" name="gst"/> 
     
     <entity name="Party">
     
     <string name="reference" title="Type" readonly="true" default="get from sequence on save"/>
     <string name="name" title="Name" required="true"/>
     <string name="type" title="Type" selection="select.party.type" required="true" default="Company"/>
     <boolean name="customer" title="Customer"/>
     <boolean name="supplier" title="Supplier"/>
     <string name="gstin" title="GSTIN" min="15" max="15"/>
     <one-to-many name="contact" ref="Contact" title="Contact"/> 
      <one-to-one ref="Address" name="address" title="Address"/>
      
     </entity>
     
    </domain-models> 